<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl11 "http://www.w3.org/2006/12/owl11#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl11xml "http://www.w3.org/2006/12/owl11-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY geophysics "http://www.integratedmodelling.org/ks/geospace/geophysics.owl#" >
    <!ENTITY representation "http://www.integratedmodelling.org/ks/science/representation.owl#" >
]>


<rdf:RDF xmlns="http://www.integratedmodelling.org/ks/geospace/geophysics.owl#"
     xml:base="http://www.integratedmodelling.org/ks/geospace/geophysics.owl"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl11="http://www.w3.org/2006/12/owl11#"
     xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
     xmlns:representation="http://www.integratedmodelling.org/ks/science/representation.owl#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:geophysics="http://www.integratedmodelling.org/ks/geospace/geophysics.owl#">
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/geospace/geospace.owl"/>
        <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/science/representation.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.integratedmodelling.org/ks/geospace/geophysics.owl#Altitude -->

    <owl:Class rdf:about="#Altitude">
        <rdfs:subClassOf rdf:resource="&representation;Length"/>
        <rdfs:comment
            >Altitude is a type of length whose use is specific to geophysical applications. By making it the observable in, e.g., an altitude raster layer, we know how to properly aggregate and distribute altitudinal data.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.integratedmodelling.org/ks/science/representation.owl#Length -->

    <owl:Class rdf:about="&representation;Length"/>
</rdf:RDF>
