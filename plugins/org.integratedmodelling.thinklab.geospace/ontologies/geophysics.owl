<?xml version="1.0"?>
<rdf:RDF
    xmlns:geophysics="http://www.integratedmodelling.org/ks/geospace/geophysics.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:representation="http://www.integratedmodelling.org/ks/science/representation.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://www.integratedmodelling.org/ks/geospace/geophysics.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/science/representation.owl"/>
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/geospace/geospace.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="GroundSurfaceTemperature">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/science/representation.owl#Temperature"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Slope">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.integratedmodelling.org/ks/science/representation.owl#GenericQuantifiable"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="http://www.integratedmodelling.org/ks/science/representation.owl#Length"/>
  <owl:Class rdf:ID="Altitude">
    <rdfs:comment>Altitude is a type of length whose use is specific to geophysical applications. By making it the observable in, e.g., an altitude raster layer, we know how to properly aggregate and distribute altitudinal data.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/science/representation.owl#Length"/>
  </owl:Class>
  <owl:Class rdf:ID="DegreeSlope">
    <rdfs:subClassOf rdf:resource="#Slope"/>
  </owl:Class>
  <owl:Class rdf:ID="GroundElevation">
    <rdfs:comment>Elevation of the highest feature on the ground, such as in LIDAR data</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://www.integratedmodelling.org/ks/science/representation.owl#Length"/>
  </owl:Class>
  <owl:Class rdf:ID="FlowConcept">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="PercentSlope">
    <rdfs:subClassOf rdf:resource="#Slope"/>
  </owl:Class>
  <owl:Class rdf:ID="FlowDirection">
    <rdfs:subClassOf rdf:resource="#FlowConcept"/>
  </owl:Class>
</rdf:RDF>

