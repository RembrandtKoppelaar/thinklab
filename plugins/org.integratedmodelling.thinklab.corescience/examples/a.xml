    xmlns:representation="http://www.integratedmodelling.org/ks/science/representation.owl#"
	xmlns:measurement="http://www.integratedmodelling.org/ks/science/measurement.owl#">
  
  <measurement:Measurement id="obs">
	  <measurement:unit>g·cm²</measurement:unit>
	  <observation:mediates>
		<measurement:Measurement id="obsGrassBiomassDensity">
			<observation:value>123 kg·m²</observation:value> 	
			<observation:hasObservable>
				<representation:MassAreaDensity id="GrassBiomass" />
			</observation:hasObservable>
		</measurement:Measurement>
	  </observation:mediates>
  </measurement:Measurement>
  
</kbox>